<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="RoundProgressBar">
        <attr name="progressInt" format="dimension"/>
        <attr name="roundColor" format="color" />
        <attr name="roundProgressColor" format="color" />
        <attr name="roundWidth" format="dimension"/>
        <attr name="txtColor" format="color" />
        <attr name="txtSize" format="dimension" />
        <attr name="max" format="integer"/>
        <attr name="startAngle" format="integer"/>
        <attr name="sweepAngle" format="integer"/>

        <attr name="textIsDisplayable" format="boolean"/>
        <attr name="style">
            <enum name="STROKE" value="0"/>
            <enum name="FILL" value="1"/>
        </attr>
    </declare-styleable>

    <declare-styleable name="ShadowLayout">
        <!--阴影颜色-->
        <attr name="shadowColor" format="color|reference" />
        <!--阴影范围/阴影半径-->
        <attr name="shadowRadius" format="dimension" />
        <!--阴影颜色在X轴偏移，正数向右，负数向左-->
        <attr name="shadowDx" format="dimension" />
        <!--阴影颜色在y轴偏移，正数向下，负数向上-->
        <attr name="shadowDy" format="dimension" />
        <!--圆角-->
        <attr name="radius" format="dimension" />
    </declare-styleable>

