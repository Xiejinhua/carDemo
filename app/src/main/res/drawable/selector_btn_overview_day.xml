<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools" tools:ignore="MissingDefaultResource">
    <item android:state_pressed="true" android:state_selected="true">
        <bitmap android:src="@drawable/ic_overview_out_day" android:alpha="0.8" />
    </item>
    <item android:state_pressed="false" android:state_selected="true">
        <bitmap android:src="@drawable/ic_overview_out_day" android:alpha="1" />
    </item>
    <item android:state_pressed="true" android:state_selected="false">
        <bitmap android:src="@drawable/ic_overview_navi_day" android:alpha="0.8" />
    </item>
    <item android:state_pressed="false" android:state_selected="false">
        <bitmap android:src="@drawable/ic_overview_navi_day" android:alpha="1" />
    </item>
</selector>