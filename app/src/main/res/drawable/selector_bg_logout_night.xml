<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape >
            <stroke android:width="@dimen/sv_dimen_1" android:color="@color/primaryContainerPressNight"/>
            <corners android:radius="@dimen/sv_dimen_15"/>
        </shape>
    </item>
    <item android:state_pressed="false">
        <shape >
            <stroke android:width="@dimen/sv_dimen_1" android:color="@color/primaryContainerNight"/>
            <corners android:radius="@dimen/sv_dimen_15"/>
        </shape>
    </item>
</selector>