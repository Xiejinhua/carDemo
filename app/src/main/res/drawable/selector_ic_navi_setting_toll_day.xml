<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true" android:state_checked="true">
        <bitmap android:src="@drawable/ic_navi_setting_toll_pressed_day" android:alpha="0.8" />
    </item>
    <item android:state_pressed="false" android:state_checked="true" android:drawable="@drawable/ic_navi_setting_toll_pressed_day"/>
    <item android:state_pressed="true" android:state_checked="false">
        <bitmap android:src="@drawable/ic_navi_setting_toll_day" android:alpha="0.8" />
    </item>
    <item android:state_pressed="false" android:state_checked="false" android:drawable="@drawable/ic_navi_setting_toll_day"/>
</selector>